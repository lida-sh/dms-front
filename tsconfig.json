{
  // https://nuxt.com/docs/guide/concepts/typescript
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "experimentalDecorators": true,
    "emitDecoratorMetadata":true,
    "strictPropertyInitialization":false,
    "noImplicitAny":false,
    "types": [
      "reflect-metadata"
    ]
  }
}

